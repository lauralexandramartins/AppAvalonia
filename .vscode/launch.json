{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Meteor Debugger",
      "type": "dotnet-meteor.debugger",
      "request": "launch",
      "preLaunchTask": "dotnet-meteor: Build"
    },
    {
      "name": "Debug MobileTeste",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
     "program": "${workspaceFolder}/MobileTeste/bin/Debug/net8.0/MobileTeste.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    }
  ]
}
